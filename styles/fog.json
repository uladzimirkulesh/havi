{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"title": "Fog",
	"settings": {
		"custom": {
			"style": {
				"inputBackgroundColor": "var(--wp--preset--color--accent)",
				"inputBorderColor": "var(--wp--preset--color--contrast-4)",
				"loadingScreenBg": "var(--wp--preset--color--contrast)",
				"menuItemHoverColor": "var(--wp--preset--color--contrast-3)",
				"submenuBackgroundColor": "var(--wp--preset--color--contrast)",
				"submenuItemColor": "var(--wp--preset--color--base-2)",
				"submenuItemHoverColor": "var(--wp--preset--color--base)",
				"overlaymenuBackgroundColor": "var(--wp--preset--color--contrast)",
				"overlaymenuItemColor": "var(--wp--preset--color--base)",
				"overlaymenuItemHoverColor": "var(--wp--preset--color--base-2)"
			}
		},
		"color": {
			"defaultPalette": false,
			"defaultGradients": false,
			"palette": [
				{
					"color": "#cdc8be",
					"name": "Accent",
					"slug": "accent"
				},
				{
					"color": "#716052",
					"name": "Accent / Two",
					"slug": "accent-2"
				},
				{
					"color": "#605043",
					"name": "Accent / Three",
					"slug": "accent-3"
				},
				{
					"color": "rgb(255, 255, 255)",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "rgba(255, 255, 255, 0.6)",
					"name": "Base / Two",
					"slug": "base-2"
				},
				{
					"color": "rgb(16, 16, 16)",
					"name": "Contrast",
					"slug": "contrast"
				},
				{
					"color": "rgba(16, 16, 16, 0.9)",
					"name": "Contrast / Two",
					"slug": "contrast-2"
				},
				{
					"color": "rgba(16, 16, 16, 0.55)",
					"name": "Contrast / Three",
					"slug": "contrast-3"
				},
				{
					"color": "rgba(16, 16, 16, 0.15)",
					"name": "Contrast / Four",
					"slug": "contrast-4"
				},
				{
					"color": "rgba(16, 16, 16, 0.12)",
					"name": "Contrast / Five",
					"slug": "contrast-5"
				},
				{
					"color": "rgba(16, 16, 16, 0.04)",
					"name": "Contrast / Six",
					"slug": "contrast-6"
				}
			]
		}
	},
	"styles": {
		"color": {
			"background": "var(--wp--preset--color--accent)"
		},
		"elements": {
			"button": {
				"color": {
					"background": "var(--wp--preset--color--accent-2)",
					"text": "var(--wp--preset--color--base)"
				},
				"border": {
					"color": "var(--wp--preset--color--accent-2)"
				},
				":hover": {
					"color": {
						"background": "var(--wp--preset--color--accent-3)",
						"text": "var(--wp--preset--color--base)"
					},
					"border": {
						"color": "var(--wp--preset--color--accent-3)"
					}
				},
				":focus": {
					"color": {
						"background": "var(--wp--preset--color--accent-3)",
						"text": "var(--wp--preset--color--base)"
					},
					"border": {
						"color": "var(--wp--preset--color--accent-3)"
					}
				},
				":active": {
					"color": {
						"background": "var(--wp--preset--color--accent-3)",
						"text": "var(--wp--preset--color--base)"
					},
					"border": {
						"color": "var(--wp--preset--color--accent-3)"
					}
				}
			}
		}
	}
}
